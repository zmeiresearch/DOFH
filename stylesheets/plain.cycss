node {
  label: data(id);
}

edge {
    line-color: #f00;
    curve-style: bezier;
    overlay-padding: 3px;
	target-arrow-color: #0f0;
    target-arrow-shape: triangle;
}

node.highlighted {
  border-width: 6px;
  border-color: #333333;
}

node.start {
  border-color: #8FDC97;
}

node.end {
  border-color: #9F4A54;
}

edge.highlighted {
  line-color: #008484;
  width: 9px;
}
